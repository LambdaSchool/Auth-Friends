iends\", friend)\n      .then((res) => console.log(\"successful res: \", res))\n      .catch((err) => console.log(err.message));\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    postFriend(newFriend);\n    setNewFriend({\n      name: \"\",\n      age: \"\",\n      email: \"\",\n      id: \"\",\n    });\n  };\n\n  return (\n    <div>\n      <h2>Add a New Friend</h2>\n      <form onSubmit={handleSubmit}>\n        <input\n          name=\"name\"\n          placeholder=\"Friend's name\"\n          value={newFriend.name}\n          onChange={handleChange}\n        />\n        <input\n          name=\"email\"\n          placeholder=\"Friend's email\"\n          value={newFriend.email}\n          onChange={handleChange}\n        />\n\n        <input\n          name=\"age\"\n          placeholder=\"Friend's age\"\n          value={newFriend.age}\n          onChange={handleChange}\n        />\n        <button>ADD FRIEND</button>\n      </form>\n    </div>\n  );\n};\n\nexport default AddFriendForm;\n","/Users/katiuska/Desktop/Lambda/Auth-Friends/friends/src/components/Dashboard.js",[],"/Users/katiuska/Desktop/Lambda/Auth-Friends/friends/src/components/Friend.js",["79"],"import React from \"react\";\nimport { useParams, useHistory } from \"react-router-dom\";\n\nconst Friend = (props) => {\n  const params = useParams();\n  const history = useHistory();\n  const goHome = () => {\n    history.push(\"/dashboard\");\n  };\n  const goToFriends = () => {\n    history.push(\"/friends\");\n  };\n\n  const friend = props.friends.find((fr) => fr.id === Number(params.id));\n\n  return (\n    <div>\n      <h3>This is the Friend component</h3>\n\n      {/* <p>Friend: {friend.name}</p> */}\n      <button onClick={goToFriends}>Back to Friends</button>\n      <br />\n      <button onClick={goHome}>Go back home</button>\n    </div>\n  );\n};\n\nexport default Friend;\n","/Users/katiuska/Desktop/Lambda/Auth-Friends/friends/src/reducers/index.js",["80"],"import {\n  GET_FRIENDS,\n  START_FRIENDS,\n  FRIENDS_SUCCESS,\n  FRIENDS_FAIL,\n} from \"../actions\";\n\nconst initialState = {\n  friends: [],\n  loading: false,\n  error: \"\",\n};\n\nexport const reducer = (state = initialState, action) => {\n  switch (action.type) {\n    case START_FRIENDS:\n      return {\n        ...state,\n        loading: true,\n        error: \"\",\n      };\n    case FRIENDS_SUCCESS:\n      return {\n        ...state,\n        loading: false,\n        error: \"\",\n        friends: action.payload,\n      };\n    case FRIENDS_FAIL:\n      return {\n        ...state,\n        loading: false,\n        error: action.payload,\n      };\n    default:\n      return state;\n  }\n};\n","/Users/katiuska/Desktop/Lambda/Auth-Friends/friends/src/actions/index.js",[],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":3,"column":10,"nodeType":"87","messageId":"88","endLine":3,"endColumn":16},{"ruleId":"85","severity":1,"message":"89","line":5,"column":10,"nodeType":"87","messageId":"88","endLine":5,"endColumn":23},{"ruleId":"90","severity":1,"message":"91","line":11,"column":6,"nodeType":"92","endLine":11,"endColumn":8,"suggestions":"93"},{"ruleId":"81","replacedBy":"94"},{"ruleId":"83","replacedBy":"95"},{"ruleId":"85","severity":1,"message":"96","line":3,"column":8,"nodeType":"87","messageId":"88","endLine":3,"endColumn":13},{"ruleId":"97","severity":1,"message":"98","line":11,"column":5,"nodeType":"99","messageId":"100","endLine":11,"endColumn":7},{"ruleId":"85","severity":1,"message":"101","line":15,"column":10,"nodeType":"87","messageId":"88","endLine":15,"endColumn":17},{"ruleId":"85","severity":1,"message":"102","line":15,"column":19,"nodeType":"87","messageId":"88","endLine":15,"endColumn":29},{"ruleId":"85","severity":1,"message":"103","line":14,"column":9,"nodeType":"87","messageId":"88","endLine":14,"endColumn":15},{"ruleId":"85","severity":1,"message":"104","line":2,"column":3,"nodeType":"87",